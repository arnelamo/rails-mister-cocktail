<div class="container">
  <h1>Cocktails</h1>
  <div id="cocktails">
    <% @cocktails.each do |cocktail| %>
    <div class="cocktail">
      <%= link_to cocktail_path(cocktail) do %>
      <h5><%= cocktail.name %></h5>
      <% end %>
    </div>
    <% end %>
    <%= link_to 'Add new cocktail', new_cocktail_path %>
  </div>
</div>
